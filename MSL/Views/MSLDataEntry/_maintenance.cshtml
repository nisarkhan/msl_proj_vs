@model MSL.Models.MaintenanceViewModel

 <div class="panel-body" ng-controller="dashboard">
    <form name="form" class="form-horizontal" ng-validate="SubmitEmployees(data)" novalidate>
        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-default">
                    <div class="panel-body">

                        <br />

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label for="" class="col-xs-6 control-label">Covered by DR Contract:</label>
                                    <div class="col-xs-6">
                                        <div class="btn-group" data-toggle="buttons">
                                           @Html.EditorFor(model => model.CoveredDRContract, new { htmlAttributes = new { @class = "form-control" } })
                                        </div> 
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label for="" class="col-xs-6 control-label">H/W Warranty Information / Contract #:</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.HWWarrantyInformationContract, new { htmlAttributes = new { @class = "form-control" } })                                        
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label for="" class="col-xs-6 control-label">Patching Group:</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.PatchingGroup, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-xs-6 control-label">Reboot Description:</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.RebootDescription, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-xs-6 control-label">S/W Support Vendor:</label>
                                    <div class="col-xs-6">
                                       @Html.EditorFor(model => model.SWSupportVendor, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <div class="row text-right">
            <div class="row text-right">
                <div class="col-xs-12">
                    <button class="btn btn-success" type="submit" id="btnSavetoDB" name="submitButton" value="Save to DB">
                        <span> <i class="fa fa-save"></i>  Save to Database </span>
                    </button>
                    &nbsp;&nbsp;&nbsp;
                    <button class="btn btn-danger" type="submit" id="btnRemoveFromGrid" name="submitButton" value="Remove Item From Grid">
                        <span> <i class="fa fa-remove"></i>  Remove </span>
                    </button>
                    &nbsp;&nbsp;&nbsp;
                </div>
            </div>
        </div>
    </form>
</div>
