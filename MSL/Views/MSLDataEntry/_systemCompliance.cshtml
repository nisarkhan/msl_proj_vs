                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.KernelPatch, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-xs-6 control-label">Memory (GB):</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.MemoryGB, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-xs-6 control-label">Prescriptive Service Level:</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.PrescriptiveServiceLevel, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-xs-6 control-label">Processor:</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.Processor, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label for="" class="col-xs-6 control-label">Initial Deployment Date:</label>
                                    <div class="col-xs-6">
                                        <div class="input-group date" ng-datepicker>
                                            @Html.EditorFor(model => model.InitialDeploymentDate, new { htmlAttributes = new { @class = "form-control" } })
                                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                        </div>
                                         
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-xs-6 control-label">RAID Level:</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.RAIDLevel, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>


