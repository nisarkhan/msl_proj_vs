                                </div>
                            </div>
                        </div>


                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-xs-6 control-label">XEN-HOST:</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.XENHOST, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div> 
