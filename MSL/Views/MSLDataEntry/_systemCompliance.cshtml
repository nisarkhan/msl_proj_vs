                                <div class="form-group">
                                    <label class="col-xs-6 control-label">Global Support Access Restriction:</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.GlobalSupportAccessRestriction, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
