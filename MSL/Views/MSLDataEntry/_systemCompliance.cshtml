                                <div class="form-group">
                                    <label class="col-xs-6 control-label">CPU Speed (in MHz):</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.CPUSpeedMHz, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
