                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-xs-6 control-label">Application/System Use:</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.ApplicationSystemUse, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-6">
                                <div class="form-group">
                                    <label class="col-xs-6 control-label">Asset Tag:</label>
                                    <div class="col-xs-6">
                                        @Html.EditorFor(model => model.AssetTag, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
